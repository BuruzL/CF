#include<stdio.h>
int main(){
    int n,m;
    scanf("%d %d", &n, &m);
    char snake[n][m];
    for(int i=0; i<n; i++){
        if((i+1)%2!=0){
            for(int j=0; j<m; j++){
                snake[i][j]='#';
            }
        }else{
            for(int j=0; j<m; j++){
                snake[i][j]='.';
            }
           if((i%4)==1)snake[i][m-1]='#';
           else snake[i][0]='#';
        }

    }
    for(int i=0; i<n; i++){
        for(int j=0; j<m; j++){
            printf("%c", snake[i][j]);
        }
        printf("\n");
    }
    
    
    return 0;
}
