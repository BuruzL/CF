#include<stdio.h>
#include<string.h>
int main(){
    int n;
    scanf("%d", &n);
    char str[n+1];
    scanf("%s", str);
    int actr=0;
    int dctr=0;
    for(int j=0; j<n; j++){
       if(str[j]=='A'){
        actr++;
       }else if(str[j]=='D'){
        dctr++;
       }
    }
    if(actr>dctr)printf("Anton");
    else if(dctr>actr)printf("Danik");
    else if(actr==dctr)printf("Friendship");

    return 0;
}
