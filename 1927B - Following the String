#include<stdio.h>
int main(){
    int n;
    scanf("%d", &n);
   
    for(int i=0; i<n; i++){
        int t;
        scanf("%d", &t);
        char str[t+1];
        int num[t];
        for(int j=0; j<t; j++){
            scanf("%d", &num[j]);
        }
        int chk[256];
        for(int k=0; k<256; k++)chk[k]=0;
        for(int j=0; j<t; j++){
           for(int k='a'; k<='z'; k++){
            if(num[j]==chk[k]){
                str[j]=(char)(k);
                chk[k]++;
                break;
            }
           }
        }
        str[t]='\0';
        printf("%s\n", str);
    }
   
    return 0;
}
