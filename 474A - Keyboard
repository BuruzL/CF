#include<stdio.h>
#include<string.h>
int main(){
    char res[]="qwertyuiopasdfghjkl;zxcvbnm,./";
    char lr;
    scanf("%c", &lr);
    char str[101];
    scanf("%100s", str);
    int len;
    len=strlen(str);
    char out[101];

    for(int i=0; i<len; i++){
        int idx=0;
       for(int j=0; j<31; j++){
        if(str[i]==res[j]){
            idx=j;
            if(lr=='R'){
                out[i]=res[idx-1];
            }else if(lr=='L'){
                out[i]=res[idx+1];
            }
        }
       }
    }
    out[len]='\0';
    for(int k=0; k<len; k++){
        printf("%c", out[k]);
    }
    return 0;

}
