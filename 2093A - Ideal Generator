#include<stdio.h>
int main(){
    int n;
    scanf("%d", &n);
    int num[n];
    int chk[n];
    for(int i=0; i<n; i++)chk[i]=0;
    for(int i=0; i<n; i++)scanf("%d", &num[i]);
    for(int i=0; i<n; i++){
        if(num[i]%2!=0)chk[i]=1;
    }
    for(int i=0; i<n; i++){
        if(chk[i]==1)printf("YES\n");
        else printf("NO\n");
    }
    return 0;
}
