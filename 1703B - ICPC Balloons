#include<stdio.h>
#include<string.h>
int main(){
    int n;
    scanf("%d", &n);
    int out[n];
    for(int i=0; i<n; i++)out[i]=0;
    for(int i=0; i<n; i++){
        int t;
        scanf("%d", &t);
        char str[t];
        scanf("%51s", str);
        int len;
        len=strlen(str);
        char chk[256];
        int sum=0;
        for(int k=0; k<256; k++)chk[k]=0;
        for(int j=0; j<len; j++){
           int b=(unsigned char)str[j];
           if(chk[b]==0){
            sum=sum+2;
            chk[b]=1;
           }else{
            sum=sum+1;
           }
        }
        out[i]=sum;
    }
    for(int i=0; i<n; i++){
        printf("%d\n", out[i]);
    }
    return 0;
}
