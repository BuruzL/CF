#include<stdio.h>
int main(){
    int n;
    scanf("%d", &n);
    char cf[n][11];
    for(int i=0; i<n; i++){
        scanf("%10s", cf[i]);
    }
    const char comp[]="codeforces";
    
    int check[n];
    for(int i=0; i<n; i++){
        check[i]=0;
    }
    for(int i=0; i<n; i++){
        for(int j=0; j<10; j++){
            if(cf[i][j]!=comp[j]){
                check[i]++;
            }
        }  
    }
    for(int i=0; i<n; i++){
        printf("%d\n", check[i]);
    }
    return 0;
}
