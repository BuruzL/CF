#include<stdio.h>
int main(){
    int n;
    scanf("%d", &n);
    int all[n];
    for(int i=0; i<n; i++){
    int s;
    scanf("%d", &s);
    int seat[s];
    for(int j=0; j<s; j++){
        scanf("%d", &seat[j]);
    }
    int vis[s+2];
    for(int k=0; k<s+2; k++) vis[k]=0;
    int ok=1;
    for(int j=0; j<s && ok; j++){
        int x=seat[j];
        if(j==0){
            vis[x]=1;
        }else{
            if(vis[x-1]|| vis[x+1]){
                vis[x]=1;
            }else{
                ok=0;
            }
        }
    }
    all[i]=ok;
    }
    for(int i=0; i<n ;i++){
        puts(all[i]? "YES" : "NO");
    }
    
    return 0;
}
