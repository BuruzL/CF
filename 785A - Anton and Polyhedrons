#include<stdio.h>
#include<string.h>
int main(){
    int n;
    scanf("%d", &n);
    int sum=0;
    char shape[100];
    for(int i=0; i<n; i++){
        scanf("%s", shape);
        if(strcmp(shape,"Tetrahedron")==0){
            sum=sum+4;
        }else if(strcmp(shape, "Cube")==0){
            sum=sum+6;
        }else if(strcmp(shape, "Octahedron")==0){
            sum=sum+8;
        }else if(strcmp(shape, "Dodecahedron")==0){
            sum=sum+12;
        }else if(strcmp(shape, "Icosahedron")==0){
            sum=sum+20;
        }
    }
    printf("%d", sum);
    return 0;
}
