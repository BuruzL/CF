#include<stdio.h>
int main(){
    int n;
    scanf("%d", &n);
    int chk[n];
    for(int i=0; i<n; i++)chk[i]=0;
    for(int i=0; i<n; i++){
        char rb[8][8];
        for(int j=0;j<8; j++){
            for(int k=0; k<8; k++){
                scanf(" %c", &rb[j][k]);
            }
        }
        for(int j=0; j<8; j++){
            int r=0;
            for(int k=0; k<8;k++){
                 if(rb[j][k]=='R')r++;
            }
            if(r==8){
                chk[i]=1;
                break;
            }
        }
        if(chk[i]==0){
            for(int k=0;k<8;k++){
                int b=0;
                for(int j=0; j<8;j++){
                    if(rb[j][k]=='B')b++;
                }
                if(b==8){
                    chk[i]=2;
                    break;
                }
            }
        }
    }
    for(int i=0; i<n; i++){
        if(chk[i]==1)printf("R\n");
        else if(chk[i]==2)printf("B\n");
    }
    return 0;
}
