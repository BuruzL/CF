#include<stdio.h>
#include<string.h>
int main(){
  char s1[101],s2[101];
  scanf("%s",s1);
  scanf("%s",s2);
  int l1,l2;
  l2=strlen(s2);
  l1=strlen(s1);
  int j=l2-1;
  int chk=1;
  if(l1!=l2)chk=0;
  for(int i=0; i<l1; i++){
    if(s1[i]!=s2[j--]){
      chk=0;
    }
  }
  if(chk==0)printf("NO\n");
  else printf("YES\n");
  return 0;
}
