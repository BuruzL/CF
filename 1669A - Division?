#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    while(n--){
        int r;
        scanf("%d",&r);
        if(r>=1900){
            printf("Division 1");
        }else  if(1600<=r && r<=1899){
            printf("Division 2");
        }else  if(1400<=r && r<=1599){
            printf("Division 3");
        }else  if(r<=1399){
            printf("Division 4");
        }

        printf("\n");
    }
    return 0;
}
