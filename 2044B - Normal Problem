#include<stdio.h>
#include<string.h>
int main(){
    int n;
    scanf("%d", &n);
    char out[n][101];
    for(int i=0; i<n; i++){
        char str[101];
        scanf("%100s", str);
        int len;
        len=strlen(str);
        str[len]='\0';
        char rev[101];
        for(int j=0; j<len; j++){
            rev[j]=str[len-1-j];
        }
        rev[len]='\0';
        for(int j=0; j<=len; j++){
            if(rev[j]=='p'){
                out[i][j]='q';
            }else if(rev[j]=='q'){
                out[i][j]='p';
            }else{
                out[i][j]=rev[j];
            }
        }
        
    }
   for(int i=0; i<n; i++){
    
        printf("%s\n", out[i]);
   
   }

    return 0;
}
