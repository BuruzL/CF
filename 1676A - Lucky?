#include<stdio.h>
int main(){
    int n;
    scanf("%d", &n);
    int chk[n];
    for(int i=0; i<n; i++)chk[i]=0;
    for(int i=0; i<n; i++){
        int num;
        scanf("%d", &num);
        int a,b;
        a=num/1000;
        b=num%1000;
        int sum1=(a%10)+(a/100)+((a/10)%10);
        int sum2=(b%10)+(b/100)+((b/10)%10);
        if(sum1==sum2)chk[i]=1;
        else chk[i]=0;
    }
    for(int i=0; i<n; i++){
       if(chk[i]==1)printf("YES\n");
       else if (chk[i]==0)printf("NO\n");
    }
    return 0;
}
