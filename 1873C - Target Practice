#include<stdio.h>
int main(){
    int n;
    scanf("%d", &n);
    int score[n];
    for(int i=0; i<n;i++)score[i]=0;
    for(int i=0; i<n; i++){
        char arr[11][11];
        int sum=0;
        for(int j=0; j<10; j++){
            for(int k=0; k<10;k++){
                scanf(" %c", &arr[j][k]);
                if(arr[j][k]=='X'){
                    int dj=j<9-j?j:9-j;
                    int dk=k<9-k?k:9-k;
                    int d=dj<dk?dj:dk;
                    sum=sum+d+1;
                }
            }
        }
      score[i]=sum;
    }
    for(int i=0; i<n; i++){
        printf("%d\n", score[i]);
    }
    return 0;
}
