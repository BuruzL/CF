#include<stdio.h>
int main(){
    int n,m;
    scanf("%d %d",&n,&m);
    int chk=1;
    for(int i=2; i*i<=n; i++){
        if(n%i==0){
            chk=0;
            break;
        }
    }
    int x=0;
    for(int i=n+1; i<50; i++){
        int p=1;
        for(int j=2; j*j<=i; j++){
            if(i%j==0){
                p=0;
                break;
            }
        }
        if(p==1){
            x=i;
            break;
        }
    }
   
    if(x==m)chk=1;
    else chk=0;

    if(chk==1)printf("YES\n");
    else printf("NO\n");
    return 0;
}
