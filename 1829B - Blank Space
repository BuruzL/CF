#include<stdio.h>
int main(){
   int n;
   scanf("%d", &n);
   int chk[n];
   for(int i=0; i<n; i++)chk[i]=0;
   for(int i=0; i<n; i++){
        int t;
        scanf("%d", &t);
        int bin[t];
        for(int j=0; j<t; j++){
            scanf("%d", &bin[j]);
        }
        int ctr=0;
        int temp=0;
        for(int j=0; j<t; j++){
            if(bin[j]==0){
                temp++;
                 if(temp>=ctr)ctr=temp;
            }
             else if(bin[j]==1){
                if(temp>=ctr)ctr=temp;
                temp=0;
             }
            
        }

        chk[i]=ctr;
   }
    for(int i=0; i<n; i++)printf("%d\n", chk[i]);
    return 0;
}
