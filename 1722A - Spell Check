#include<stdio.h>
#include<string.h>
int main(){
   int n;
   scanf("%d", &n);
    int check[n];
    for(int i=0; i<n; i++)check[i]=0;
   for(int i=0;i<n;i++){
    int t;
    scanf("%d", &t);
    char s[t+1];
    scanf("%s", s);
    if(t==5){
        int freq[256]={0};
        for(int j=0; j<5; j++){
            freq[(unsigned char)s[j]]++;

            if(freq['T']==1 && freq['i']==1 && 
            freq['m']==1 && freq['u']==1 && freq['r']==1){
                check[i]=1;
            }
        }
    }
   }
   for(int k=0; k<n; k++){
    if(check[k]==1){
        printf("YES\n");
    }else{
        printf("NO\n");
    }
   }
   

    return 0;
}
