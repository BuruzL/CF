#include<stdio.h>
int main(){
    int n;
    scanf("%d", &n);
    while(n--){
        int t;
        scanf("%d",&t);
        int num[t];
        for(int i=0; i<t; i++){
            scanf("%d",&num[i]);
        }
        int max=-1;
        
        for(int i=0; i<t; i++){
            if(num[i]>max){
                max=num[i];
            }
        }
        
            printf("%d\n",max);
        
    }
    return 0;
}
