#include<stdio.h>
int main(){
    int n; 
    scanf("%d",&n);

    if(n>=1 && n<=90){
        int num[n];
        for(int i=0; i<n;i++){
            scanf("%d",&num[i]);
        }

        int output[n];
        for(int j=0;j<n;j++){
           
                output[j]=num[j]%10+num[j]/10;
        }

        for(int k=0; k<n; k++){
            printf("%d\n",output[k]);
        }
    }

    return 0;
}
