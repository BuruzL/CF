#include<stdio.h>
#include<string.h>
int main(){
    int n;
    scanf("%d", &n);
   char s[200005], t[200005], out[200005];
    while(n--){
        
        scanf("%s %s",s,t);
        int cs[26]={0}, ct[26]={0};
         for (int i = 0; s[i]; ++i) cs[s[i]-'A']++;
        for (int i = 0; t[i]; ++i) ct[t[i]-'A']++;

        int ok = 1;
        for (int c = 0; c < 26; ++c)
            if (ct[c] > cs[c]) { ok = 0; break; }

            if (!ok) { puts("NO"); continue; }
             int need[26];
        for (int c = 0; c < 26; ++c) need[c] = cs[c] - ct[c];

         int p = 0;
        for (int i = 0; s[i]; ++i) {
            int c = s[i] - 'A';
            if (need[c] > 0) need[c]--;     
            else out[p++] = s[i];   
        }        
        out[p] = '\0';
         puts(strcmp(out, t) == 0 ? "YES" : "NO");

    }
    return 0;
}
