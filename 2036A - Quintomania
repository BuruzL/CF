#include<stdio.h>
#include<stdlib.h>
int main(){
    int n;
    scanf("%d", &n);
    int chk[n];
    for(int i=0; i<n; i++)chk[i]=0;
    for(int i=0; i<n; i++){
        int t;
        scanf("%d", &t);
        int f=1;
        int num[t];
        for(int j=0; j<t; j++){
            scanf(" %d", &num[j]); 
        }
        for(int k=0; k<t-1; k++){
            int d = abs(num[k + 1] - num[k]);
            if(d!=5 && d!=7){
                f=0;
                break;
            }
        }

        if(f==1)chk[i]=1;
    }
    for(int i=0; i<n; i++){
        if(chk[i]==0)printf("NO\n");
        else if(chk[i]==1)printf("YES\n");
    }
    return 0;
}
