#include<stdio.h>
#include<string.h>
int main(){
    int n;
    scanf("%d", &n);
    while(n--){
        char str[101];
        scanf("%100s", str);
        int len;
        len=strlen(str);
        if(len%2!=0){
            printf("NO\n");
        }else{
           char str1[101];
           char str2[101];
           int d=len/2;
           for(int i=0; i<d; i++){
            str1[i]=str[i];
           }str1[d]='\0';
           int k=0;
           for(int i=d; i<len; i++){
            str2[k++]=str[i];
           }str2[k]='\0';
           if(strcmp(str1, str2)==0){
            printf("YES\n");
           }
            else{
                printf("NO\n");
            }
           }
        
    }
    return 0;
}
