#include<stdio.h>
int main(){
    int t;
    int n;
    scanf("%d", &t);
    int num[t][3];
    int check[t];
    for(int i=0; i<t; i++){
        for(int j=0; j<3; j++){
            scanf("%d", &num[i][j]);
        }
    }
    for(int i=0; i<t; i++){
        check[i]=0;
    }
    for(int i=0; i<t; i++){
        if((num[i][0]+num[i][1])==num[i][2]){
            check[i]=1;
        }
        else if((num[i][1]+num[i][2])==num[i][0]){
            check[i]=1;
        }
        else if((num[i][0]+num[i][2])==num[i][1]){
            check[i]=1;
        }
        else{
            check[i]=0;
        }
    }

    for(int i=0; i<t; i++){
        if(check[i]==1){
            printf("YES\n");
        }
        else{
            printf("NO\n");
        }
    }
   
    return 0;
}
