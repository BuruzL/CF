#include<stdio.h>
int main(){
    int n;
    scanf("%d", &n);
    int chk[n];
    for(int i=0; i<n; i++)chk[i]=0;
    for(int i=0; i<n; i++){
        int a,b,c;
        scanf("%d%d%d",&a, &b, &c);
        if(a<b && b<c){
            chk[i]=1;
        }else if(a<b && b>c){
            chk[i]=2;
        }else{
            chk[i]=0;
        }

    }
    for(int i=0; i<n; i++){
        if(chk[i]==1)printf("STAIR\n");
        else if(chk[i]==2)printf("PEAK\n");
        else printf("NONE\n");
    }
}
