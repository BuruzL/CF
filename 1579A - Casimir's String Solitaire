#include<stdio.h>
#include<string.h>
int main(){
    int n;
    scanf("%d", &n);
    int chk[n];
    for(int i=0; i<n; i++)chk[i]=0;
    for(int i=0; i<n; i++){
        char str[51];
        scanf("%50s", str);
        int len;
        len=strlen(str);
        if(len%2!=0){
            chk[i]=0;
        }else{
            int b=0,ac=0;
            for(int j=0; j<len; j++){
                if(str[j]=='A' || str[j]=='C')ac++;
                else b++;
            }
            if(b==ac){
                chk[i]=1;
            }else{
                chk[i]=0;
            }
        }
    }
    for(int i=0; i<n; i++){
        if(chk[i]==1)printf("YES\n");
        else printf("NO\n");
    }
    return 0;
}
